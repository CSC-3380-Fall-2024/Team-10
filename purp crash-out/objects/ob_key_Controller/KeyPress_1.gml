var newKey = keyboard_key;

if (newKey != vk_escape)
{
	case "up":
		obj_key_up.key = newKey;
	break;
	case "down":
		obj_key_down.key = newKey;
	break;
	case "left":
		obj_key_left.key = newKey;
	break;
	case "right":
		obj_key_right.key = newKey;
	break;
}
else
{
	instance_destroy();
}
